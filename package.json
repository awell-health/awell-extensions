{
  "name": "@awell/action-extensions",
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "dev": "PRETTY_LOGS=true LOG_LEVEL=debug ts-node-dev --clear --transpile-only ./src/index.ts",
    "clean": "rimraf dist/*",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "lint": "eslint **/*.ts",
    "postinstall": "husky install",
    "test": "jest",
    "test:publish-new-activity": "ts-node-dev --transpile-only ./src/publish-new-activity.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "pino-pretty": "^9.4.0",
    "prettier": "2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.3.0",
    "fastify": "^4.13.0",
    "lightship": "^7.1.1",
    "lodash": "^4.17.21",
    "twilio": "^4.7.2"
  }
